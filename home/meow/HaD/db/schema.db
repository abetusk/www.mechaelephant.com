create table post (
  post_id int primary key not null,
  date datetime,
  date_gmt datetime,
  guid varchar,

  modified datetime,
  modified_gmt datetime,
  slug varchar,
  type varchar,
  link varchar,
  title varchar,
  content varchar,
  excerpt varchar,
  author int,
  featured_media int,
  -- comment_status varchar,
  -- sticky int,
  -- format varchar,
  -- meta_jetpack_publicize_message varchar,
  -- meta_jetpack_is_tweetstorm int,
  -- meta_jetpack_publicise_feature_enabled int,

  -- jetpack_featured_media_url varchar,
  -- jetpack_shortlink varchar,

  category_post_id int,
  tag_post_id int


);

create index idx_post_id on post (post_id);
create index idx_post_date on post (date);
create index idx_post_date_gmt on post (date_gmt);
create index idx_post_slug on post (slug);

create table tag_post (
  tag_post_id int primary key not null,
-- tag_name varchar,
  post_id int not null,
  tag_id int not null
);

create index idx_tag_post_id on tag_post (tag_post_id);
create index idx_tag_post_tid on tag_post (tag_id);
-- create index idx_tag_post_name on tag_post (tag_name);

create index idx_tag_post_id_tid on tag_post (tag_post_id, tag_id);
-- create index idx_tag_post_id_name on tag_post (tag_post_id, tag_name);

create table category_post (
  category_post_id int primary key not null,
-- category_name varchar,
  post_id int not null,
  category_id int not null
);

create index idx_category_post_id on category_post (category_post_id);
create index idx_category_post_cid  on category_post (category_id);
--create index idx_category_post_name on category_post (category_name);

create index idx_category_post_id_cid on category_post (category_post_id, category_id);
-- create index idx_category_post_id_name on category_post (category_post_id, category_name);

create table tag (
  tag_id int primary key not null,
  count int,
  description varchar,
  link varchar,
  name varchar,
  slug varchar,
  taxonomy varchar,

  self_url varchar,
  collection_url varchar,
  about_url varchar,
  wp_post_type_url varchar

);

create index idx_tag_id on tag (tag_id);
create index idx_tag_name on tag (name);
create index idx_tag_link on tag (link);
create index idx_tag_slug on tag (slug);

create table category (
  category_id int primary key,
  count int,
  description varchar,
  link varchar,
  name varchar,
  slug varchar,
  taxonomy varchar
);

create index idx_category_id on category (category_id);
create index idx_category_name on category (name);
create index idx_category_link on category (link);
create index idx_category_slug on category (slug);


